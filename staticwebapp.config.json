{
  "routes": [
    {
      "route": "/",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/caso-exploracion-minera",
      "serve": "/caso-exploracion-minera.html",
      "statusCode": 200
    },
    {
      "route": "/caso-ingenieria-civil",
      "serve": "/caso-ingenieria-civil.html",
      "statusCode": 200
    },
    {
      "route": "/caso-recursos-hidricos",
      "serve": "/caso-recursos-hidricos.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*.{jpg,jpeg,png,gif,svg,webp,tif,tiff}",
      "/*.{css,js,json,ico}",
      "/favicon.ico"
    ]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".html": "text/html",
    ".css": "text/css",
    ".js": "application/javascript",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".jpeg": "image/jpeg",
    ".gif": "image/gif",
    ".svg": "image/svg+xml",
    ".webp": "image/webp",
    ".tif": "image/tiff",
    ".tiff": "image/tiff",
    ".ico": "image/x-icon"
  },
  "globalHeaders": {
    "Cache-Control": "public, max-age=31536000"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}
